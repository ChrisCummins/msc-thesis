#!/usr/bin/env bash
#
# Wraps a functions to generate plots and visuals for PPar conference
# poster.
#

set -ex

./plot_coverage
cp img/best_wg_distribution.png "$HOME/Dropbox/ed/University/1 msc/conf"
cp img/best_wg_distribution.png "$HOME/Dropbox/ed/University/1 msc/conf"
cp pos_freq.txt "$HOME/Dropbox/ed/University/1 msc/conf"

cd ~/src/msc-thesis/benchmarks/
./eval_e14
cp ~/src/msc-thesis/benchmarks/test_speedups.txt "$HOME/Dropbox/ed/University/1 msc/conf"
